    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "@supabase/supabase-js": "^2.38.4"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "concurrently": "^8.2.2"
